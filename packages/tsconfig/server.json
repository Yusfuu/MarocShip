{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Server",
  "extends": "./base.json",
  "compilerOptions": {
    "sourceMap": true,
    "target": "ESNext",
    "module": "CommonJS",
    "outDir": "build",
    "strict": true,
    "lib": ["esnext"],
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "incremental": true
  }
}
